package templates

templ Home(path string) {
	<!DOCTYPE html>
	<html>
		<head>
			<link rel="stylesheet" href={path}/>
			<script src="https://unpkg.com/htmx.org@1.9.12"></script>
			<title>GPA</title>
		</head>

		<body>
			<button id="start" hx-post="/form" hx-target="div.semesterlist" hx-swap="beforeend">
				Render Form
			</button>
			<div class="semesterlist">
			</div>
		</body>
	</html>
}
